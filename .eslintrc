{
	"env":{
		"es6": true,
		"browser" : true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-console": 0,
		"indent": [
			2,
			"tab"
		],
		"linebreak-style": [
			2,
			"linux"
		],
		"quotes": [
			2,
			"single"
		],
		"semi": [
			2,
			"always"
		]
	}
}
